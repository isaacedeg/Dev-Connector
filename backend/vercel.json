{
    "version": 2,
    "name": "backend",
    "builds": [{ "src": "server.js", "use": "@vercel/node" }],
    "routes": [
      { "src": "/(.*)", 
        "dest": "/server.js", 
        "methods": ["GET"] 
      },
      {
        "src": "/api/profile",
        "methods": ["POST", "GET", "PUT", "DELETE"],
        "dest": "/routes/api/profile.js"
      }
    ]
  }